-- Scripts:

local function OZGWOJJ_fake_script() -- System.Controller 
	local script = Instance.new('LocalScript', System)

	local TeleportService = game:GetService("TeleportService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	
	local GUI = script.Parent
	local KeySystem = GUI:WaitForChild("KeySystem")
	local CheckButton = KeySystem:WaitForChild("Check")
	local GetKeyButton = KeySystem:WaitForChild("Retrieve")
	local KeyTextbox = KeySystem:WaitForChild("Key")
	local KeyBackground = KeySystem:WaitForChild("Background")
	
	--[[                    --------------------------------
	CODE IMPORTANT BELOW
	
	
	
	]]
	local ACCESS_CODE = "13579123"
	--[[
	
	
	
	CODE IMPORTANT ABOVE
	]]                      --------------------------------
	local RandomBackgrounds = {"rbxassetid://8153824697", "rbxassetid://10056462700", "rbxassetid://17858351415", "rbxassetid://911295583","rbxassetid://9083332688"}
	KeyBackground.Image = RandomBackgrounds[math.random(1,#RandomBackgrounds)]
	
	local Main = GUI:WaitForChild("Main")
	local Autofarm_Button = Main:WaitForChild("AutoFarmButton")
	local ServerHop_Buttn = Main:WaitForChild("ServerHopButton")
	local Mute_Button = Main:WaitForChild("MuteButton")
	local SilentAim_Button = Main:WaitForChild("SilentButton")
	
	if _G.RunningGui then _G.RunningGui:Destroy() end
	
	_G.RunningGui = GUI
	
	_G.AutofarmScriptRunning = _G.AutofarmScriptRunning or false
	_G.AutofarmFuncsRan = _G.AutofarmFuncsRan or false
	
	_G.GuiSafetyCodeRunning = _G.GuiSafetyCodeRunning or false
	
	local AutofarmClickCooldown = false
	
	function SendNotification(GTitle, GText, GIcon, GDuration, GButton, Button1Callback)
		
		if not Button1Callback then		
			game.StarterGui:SetCore("SendNotification", {
				Title = GTitle;
				Text = GText;
				Icon = GIcon;
				Duration = GDuration;
				Button1 = GButton;
			})
		else
			local bindable = Instance.new("BindableFunction")
	
			-- When the button is pressed, Roblox will call this function
			function bindable.OnInvoke(response)
				if response == GButton and typeof(Button1Callback) == "function" then
					Button1Callback()
				end
			end
			
			game.StarterGui:SetCore("SendNotification", {
				Title = GTitle;
				Text = GText;
				Icon = GIcon;
				Duration = GDuration;
				Button1 = GButton;
				Callback = bindable;
			})
			
		end
	end
	
	function ServerHop()
		local placeId = game.PlaceId
		TeleportService:Teleport(placeId)
	end
	
	function CheckForAdmins()
		local FoundHigherAuthority = false
	
		for i, OtherPlayer in game.Players:GetPlayers() do
			if OtherPlayer.Name == LocalPlayer.Name then continue end
			if OtherPlayer:IsInGroup(7513662) then
				local rank = OtherPlayer:GetRoleInGroup(7513662)
				if rank ~= "Fans" then
					FoundHigherAuthority = true
					break
				end
			end	
		end
	
		if FoundHigherAuthority then ServerHop() end
	end
	
	function GUIFunction()
		Main.Visible = true
		Autofarm_Button.MouseButton1Click:Connect(function()
			if AutofarmClickCooldown then return end
			AutofarmClickCooldown = true
			spawn(function()
				wait(3)
				AutofarmClickCooldown = false
			end)
			
			if LocalPlayer.Character:FindFirstChild("Humanoid") then
				_G.AutofarmScriptRunning = not _G.AutofarmScriptRunning
				LocalPlayer.Character:FindFirstChild("Humanoid").Health = 0
			else
				return
			end
			
			if _G.AutofarmScriptRunning then
				Autofarm_Button.Text = "AUTOFARM STATUS : ON"
				if not _G.AutofarmFunsRan then
					_G.AutofarmFunsRan = true
					spawn(function()
	
						local Players = game:GetService("Players")
						local LocalPlayer = Players.LocalPlayer
	
						_G.PhonkSounds = {
							[1] = {Audio = "rbxassetid://107424491541808", Length = 136}, -- phonk1
							[2] = {Audio = "rbxassetid://94017327845820", Length = 97}, -- tung tung
							[3] = {Audio = "rbxassetid://89907278904871", Length = 77}, -- phonk2
							[4] = {Audio = "rbxassetid://136348083756891", Length = 85}, -- labubu
							[5] = {Audio = "rbxassetid://103283136727523", Length = 160}, -- ultra chill phonk
							[6] = {Audio = "rbxassetid://116979755785496", Length = 74}, -- tikitaka phonk
						}
	
						-- Globals
						_G.SoundEndTime = _G.SoundEndTime or 0
						_G.LoopingPhonk = _G.LoopingPhonk or false
	
						local function playRandomSound()
							local chosen = _G.PhonkSounds[math.random(1, #_G.PhonkSounds)]
	
							local args = {
								"PlaySound",
								{
									"Equip",
									"Roblox",
									chosen.Audio,
									{{game:GetService("ReplicatedStorage").GameContent.Crates.CrateT2_HW_BM.ShopModel}}, -- Parent of Sound
									10000000000000000000000 -- Volume
								}
							}
	
							local Knife = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Knife") 
								or LocalPlayer.Backpack:FindFirstChild("Knife")
	
							if Knife and _G.AutofarmScriptRunning then
								Knife:WaitForChild("ClientEvent"):FireServer(unpack(args))
								
								--LOCAL SOUND
								spawn(function()
									local ClientSound = Instance.new("Sound", game:GetService("ReplicatedStorage").GameContent.Crates.CrateT2_HW_BM.ShopModel)
									ClientSound.SoundId = chosen.Audio
									ClientSound.Volume = 10000000000000000000000
									ClientSound.SoundGroup = game:GetService("SoundService").MasterSound
									ClientSound:Play()
									wait(chosen.Length)
									ClientSound:Destroy()
								end)
								
								
								_G.SoundEndTime = tick() + chosen.Length
							end
						end
	
	
						-- Global loop
						if not _G.LoopingPhonk then
							_G.LoopingPhonk = true
							task.spawn(function()
								while _G.LoopingPhonk do
									if tick() >= _G.SoundEndTime then
										playRandomSound()
									end
									task.wait(1)
								end
							end)
						end
	
	
						if _G.AutofarmScriptRunning then
							local Anim1 = Instance.new("Animation")
							Anim1.AnimationId = "rbxassetid://282574440"
							local track1 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim1)
							track1:Play(.1, 1, 1)
	
							local Anim2 = Instance.new("Animation")
							Anim2.AnimationId = "rbxassetid://126753849"
							local track2 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim2)
							track2.Priority = Enum.AnimationPriority.Action4
							track2.Looped = true
							track2:Play(.1, 1, 3)
	
							game.Players.LocalPlayer.Character.Humanoid.HipHeight = 5
						end
	
	
						-- Respawn detection
						LocalPlayer.CharacterAdded:Connect(function()
							if not _G.AutofarmScriptRunning then return end
							task.wait(1) -- let character load
							-- Don’t reset timer — just try replaying if song is still supposed to be running
							if tick() < _G.SoundEndTime then
								playRandomSound()
							end
	
							spawn(function()
								local Anim1 = Instance.new("Animation")
								Anim1.AnimationId = "rbxassetid://282574440"
								local track1 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim1)
								track1:Play(.1, 1, 1)
	
								local Anim2 = Instance.new("Animation")
								Anim2.AnimationId = "rbxassetid://126753849"
								local track2 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim2)
								track2.Priority = Enum.AnimationPriority.Action4
								track2.Looped = true
								track2:Play(.1, 1, 3)
	
								game.Players.LocalPlayer.Character.Humanoid.HipHeight = 5
							end)
	
						end)
					end)
	
					spawn(function()
						local LocalPlayer = game.Players.LocalPlayer
	
						local function KillLoop()
							spawn(function()
								while _G.AutofarmScriptRunning do task.wait() 
	
									local function CanTeleportTo(plr)
										if not plr then return false end
										local char = plr.Character
										if not (char and char:FindFirstChild("Humanoid") and char:FindFirstChild("HumanoidRootPart")) then return false end
										if char.Humanoid.Health <= 0 then return false end
										if char:FindFirstChild("ForceField") then return false end
										local appearance = char:FindFirstChild("AppearanceLoaded")
										if not (appearance and appearance.Value == true) then return false end
										local HasWeapon = false
										if plr.Backpack:FindFirstChild("Knife") or plr.Backpack:FindFirstChild("Revolver") then HasWeapon = true end 
										if not HasWeapon then return false end
										return true
									end
	
									local function TeleportTo(plr)
	
										local HRP = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
										local rootPart = plr.Character.HumanoidRootPart
										HRP.CFrame = rootPart.CFrame - rootPart.CFrame.LookVector * 4.5 + Vector3.new(0, 6, 0)
										rootPart.Size = Vector3.new(15, 15, 15)
										rootPart.Transparency = 1
									end
	
									for i, Player in game.Players:GetPlayers() do
										if Player.Name == LocalPlayer.Name then continue end
										if CanTeleportTo(Player) and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
											TeleportTo(Player)
											if _G.KillRemoteCooldown then break end
											local Knife = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Knife") or LocalPlayer.Backpack:FindFirstChild("Knife")
											if Player.Character:FindFirstChild("Humanoid") and Player.Character.Humanoid.Health ~= 0 and Knife then
	
												local args = {
													"StartCharge",
													{}
												}
												Knife:WaitForChild("ClientEvent"):FireServer(unpack(args))
												local HRP = LocalPlayer.Character.HumanoidRootPart
												local HRP2 = Player.Character.HumanoidRootPart
	
												for _, obj in ipairs(getgc(true)) do
													if type(obj) == "table" and rawget(obj, "attackIDCounter") then
														--print("Found variable:", obj.attackIDCounter)
														obj.attackIDCounter = obj.attackIDCounter + 1
														_G.ScriptAttackID = obj.attackIDCounter
													end
												end
												local args1 = {
													"ChargeRelease",
													{
														0,
														true,
														--vector.create(HRP.Position.X,HRP.Position.Y,HRP.Position.Z),
														vector.create(HRP2.Position.X, HRP2.Position.Y, HRP2.Position.Z),
														vector.create(HRP2.Position.X, HRP2.Position.Y, HRP2.Position.Z),
														{},
														_G.ScriptAttackID
													}
												}
												Knife:WaitForChild("ClientEvent"):FireServer(unpack(args1))
	
												local args2 = {
													{
														DamageType = "Knife",
														attackID = _G.ScriptAttackID,
														IsHeadshot = false,
														HitVelocity = vector.create(0,100,0),
														Damage = 100,
														WeaponSkin = "ShovelFortress",
														AttackType = "KnifeThrow",
														PowerName = "DefaultKnifePower",
														TargetHumanoid = HRP2.Parent.Humanoid,
														hitPart = "Head",
														HitPoint = HRP2.CFrame,
														TargetCharacter = HRP2.Parent
													}
												}
												game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("Gameplay"):WaitForChild("DamageRequest"):FireServer(unpack(args2))
	
												local args = {
													"PlaySound",
													{
														"Equip",
														"Roblox",
														"rbxassetid://120959566661842",
														{HRP}, -- Parent of Sound
														1000000000000000000 -- Volume
													}
												}
	
												if Knife then
													
													--LOCAL SOUND
													spawn(function()
														local ClientSound = Instance.new("Sound", HRP)
														ClientSound.SoundId = "rbxassetid://120959566661842"
														ClientSound.Volume = 1000000000000000000
														ClientSound.SoundGroup = game:GetService("SoundService").MasterSound
														ClientSound:Play()
														wait(4)
														ClientSound:Destroy()
													end)
													
													Knife:WaitForChild("ClientEvent"):FireServer(unpack(args))
												end
	
	
	
												_G.KillRemoteCooldown = true
												spawn(function()
													wait(0.25)
													_G.KillRemoteCooldown = false
												end)
											end
	
										end
	
									end 
	
								end
							end)
						end
	
						if _G.AutofarmScriptRunning then
							KillLoop()
						end
	
	
						LocalPlayer.CharacterAdded:Connect(function()
							if _G.AutofarmScriptRunning then
								KillLoop()
							end
						end)
	
					end)
	
				end
			else
				Autofarm_Button.Text = "AUTOFARM STATUS : OFF"
			end
		end)
	
		Mute_Button.MouseButton1Click:Connect(function()
			if game:GetService("SoundService").MasterSound.Volume then
				if game:GetService("SoundService").MasterSound.Volume == 0 then
					game:GetService("SoundService").MasterSound.Volume = 0.5
				else
					game:GetService("SoundService").MasterSound.Volume = 0
				end
			end
		end)
		
		SilentAim_Button.MouseButton1Click:Connect(function()
			spawn(function()
				local UserInputService = game:GetService("UserInputService")
	
				local cooldown = false
	
				local function canSeeTarget(originPart, targetPart)
					local rayParams = RaycastParams.new()
					rayParams.FilterType = Enum.RaycastFilterType.Blacklist
					rayParams.FilterDescendantsInstances = {LocalPlayer.Character}
	
					local result = workspace:Raycast(originPart.Position, (targetPart.Position - originPart.Position), rayParams)
					if result then
						-- If the hit part is not part of the target's character, then blocked
						if not result.Instance:IsDescendantOf(targetPart.Parent) then
							return false
						end
					end
					return true
				end
	
				local function getClosestPlayer()
					local closestPlayer, closestDistance
					local char = LocalPlayer.Character
					if not char or not char:FindFirstChild("HumanoidRootPart") or not char:FindFirstChild("Head") then return nil end
					local myPos = char.HumanoidRootPart.Position
	
					for _, player in ipairs(Players:GetPlayers()) do
						if player ~= LocalPlayer 
							and player.Character
							and player.Character:FindFirstChild("HumanoidRootPart")
							and player.Character:FindFirstChild("Humanoid")
							and not player.Character:FindFirstChild("ForceField")
							and player.Character.Humanoid.Health > 0 then
	
							local dist = (player.Character.HumanoidRootPart.Position - myPos).Magnitude
							if not closestDistance or dist < closestDistance then
								-- Check line of sight
								if canSeeTarget(char.Head, player.Character.Head) then
									closestDistance = dist
									closestPlayer = player
								end
							end
						end
					end
					return closestPlayer
				end
	
				local function ReloadUpdate(val)
					local char = LocalPlayer.Character
					if char and char:FindFirstChild("Revolver") then
						char.Revolver:FindFirstChild("ClientEvent"):FireServer("ReloadUpdate", { val })
						if val == "Start" then
							wait(0.75)
							ReloadUpdate("AmmoAdded") 
						end
					end
				end
	
				if _G.ScriptConnection then _G.ScriptConnection:Disconnect() end
	
				_G.ScriptConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
					if gameProcessed then return end
					if input.KeyCode == Enum.KeyCode.E then
						if cooldown then return end
	
						local char = LocalPlayer.Character
						if char and char:FindFirstChild("Revolver") then
							-- Find closest valid target
							local target = getClosestPlayer()
							if not target then
								--warn("No valid target found or line of sight blocked")
								return
							end
							spawn(function()
								cooldown = true
								--wait(0.55)
								wait(0.75)
								cooldown = false
							end)
							-- === Original Script ===
							for _, obj in ipairs(getgc(true)) do
								if type(obj) == "table" and rawget(obj, "attackIDCounter") then
									--print("Found variable:", obj.attackIDCounter)
									obj.attackIDCounter = obj.attackIDCounter + 1
									_G.ScriptAttackID = obj.attackIDCounter
								end
							end
	
							local Head = char:WaitForChild("Head")
							local pos2 = Head.Position
	
							spawn(function()
								local hrp = target.Character:FindFirstChild("HumanoidRootPart")
								if not hrp then return end
								local pos = hrp.Position
								local direction = (pos - pos2).Unit
								local HeadshotOrNah = false
								if math.random(1,10) == 1 then
									HeadshotOrNah = true
								end
								spawn(function()
									local function Shoot(arg1, arg2, arg3)
										local v_start = arg1.Start
										local targetPos = arg1.Target
	
										-- Do the raycast
										local ignoreList = arg2.GenerateIgnoreList({ game.Players.LocalPlayer.Character })
										local hitPart, hitPos = arg2.RayCast(v_start, (CFrame.new(v_start, targetPos) * CFrame.new(0, 0, -1)).p, 900, ignoreList)
	
										-- Use hitPos or fallback to target
										if not hitPos then
											hitPos = targetPos
										end
	
										-- Create tracer part
										local tracer = arg2.GenInst("Part", {
											["BrickColor"] = BrickColor.new("Lily white"),
											["Material"] = "Plastic",
											["Transparency"] = 0.4,
											["Anchored"] = true,
											["CanCollide"] = false,
											["Name"] = "Tracer",
											["Size"] = Vector3.new(0.2, 0.2, 0.2),
											["Parent"] = arg1.ProjectileDebris
										})
	
										-- Add cylinder mesh for the tracer beam
										local mesh = arg2.GenInst("CylinderMesh", {
											["Parent"] = tracer,
											["Name"] = "BeamMesh"
										})
	
										-- Position tracer between start and hitPos
										local distance = (v_start - hitPos).Magnitude
										tracer.CFrame = CFrame.new(v_start, hitPos) * CFrame.new(0, 0, -distance / 2) * CFrame.Angles(math.pi/2, 0, 0)
										mesh.Scale = Vector3.new(1, distance * 5, 1)
	
										-- Fade and clean up
										arg2.FadeClean(tracer, 0, 0.2)
	
									end
	
									local Table = {
										["Start"] = pos2,
										["Target"] = pos,
										["ProjectileDebris"] = workspace.WorldIgnore.Projectiles
									}
	
									Shoot(Table,require(game:GetService("Players").LocalPlayer.PlayerGui.GameUI.ClientMaster.CommonFunctions))
									spawn(function()
										local ShootSound = Instance.new("Sound",workspace)
										ShootSound.SoundId = "rbxassetid://3954780669"
										ShootSound:Play()
										wait(1.6)
										ShootSound:Destroy()
									end)
	
								end)
	
								local args = {
									{
										AttackType = "KnifeThrow",
										attackID = _G.ScriptAttackID,
										IsHeadshot = HeadshotOrNah,
										WeaponOriginalUser = true,
										HitVelocity = -direction * 0.5,
										WeaponSkin = "StockRevolver",
										Damage = 100,
										DamageType = "Revolver",
										TargetHumanoid = target.Character:WaitForChild("Humanoid"),
										hitPart = "Head",
										WeaponClass = "RevolverClass",
										TargetCharacter = target.Character
									}
								}
								game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("Gameplay"):WaitForChild("DamageRequest"):FireServer(unpack(args))
	
								local args2 = {
									"WeaponFired",
									{
										vector.create(pos2.X, pos2.Y, pos2.Z),
										vector.create(pos.X, pos.Y, pos.Z),
										{
											hitOffset = CFrame.new(),
											hasHitPlayer = false,
											replicatedHitPos = vector.create(pos.X, pos.Y, pos.Z),
											hasHitPart = true,
											fireOffset = CFrame.new()
										},
										_G.ScriptAttackID
									}
								}
								char.Revolver:WaitForChild("ClientEvent"):FireServer(unpack(args2))
	
								ReloadUpdate("Start")
	
							end)
						else
							--warn("Revolver not found in your character!")
						end
					end
				end)
	
			end)
			SendNotification("Dabitob","click e with ur gun out twin","rbxassetid://12386566164",10,"ok")
		end)
	
		ServerHop_Buttn.MouseButton1Click:Connect(function()
			ServerHop()
		end)
	
	
